#Restaurant entity
CREATE TABLE Restaurant (id bigint GENERATED BY DEFAULT AS IDENTITY, peanutIndividualScore int, peanutTotalScore int, peanutAverageScore float, eggIndividualScore int, eggTotalScore int, eggAverageScore float, dairyIndividualScore int, dairyTotalScore int, dairyAverageScore float, totalAverageScore float, PRIMARY KEY (id));
DROP TABLE Restaurant IF EXISTS;

DiningReview entiry
CREATE TABLE DiningReview (id bigint GENERATED BY DEFAULT AS IDENTITY, Restaurantid bigint NOT NULL, name varchar(255), peanutScore int, eggScore int, dairyScore int, comment varchar(255), PRIMARY KEY (id));
DROP TABLE DiningReview IF EXISTS;
